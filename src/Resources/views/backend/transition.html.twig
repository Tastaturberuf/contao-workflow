{% extends "@ContaoCore/Backend/be_page.html.twig" %}

{% block headline %}
    {{ headline|raw }}
{% endblock %}

{% block error %}
{% if errors %}
    {% for error in errors %}
        {{ error }}<br>
    {% endfor %}
{% endif %}
{% endblock %}

{% block main %}
<div id="tl_buttons">
    <a href="javascript:history.back();" class="header_back" title="">Zur√ºck</a>
</div>

<div class="workflow-section">
    <h2 class="sub_headline">{{ subheadline }}</h2>

    <div class="workflow-content-box">
        <p>{{ transition.configValue('description') }}</p>
    </div>

    {% form_theme form '@NetzmachtContaoForm/form/contao_backend.html.twig' %}
    {{ form(form) }}
</div>

{% endblock %}
