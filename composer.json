{
    "name":"netzmacht/contao-workflow",
    "description":"Library for supporting customized Contao form rendering",
    "keywords":["contao", "workflow", "data flow"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"https://netzmacht.de"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/contao-workflow/issues",
        "source":"https://github.com/netzmacht/contao-workflow"
    },
    "require":{
        "php":">=7.1",
        "contao/core-bundle":"~4.4",
        "contao-community-alliance/meta-palettes": "~1.10",
        "menatwork/contao-multicolumnwizard": "^3.3.14",
        "beberlei/assert": "~2.0",
        "netzmacht/workflow": "~2.0@dev",
        "netzmacht/contao-toolkit": "~3.0@beta",
        "mathiasverraes/classfunctions": "~1.1",
        "symfony/expression-language": "~2.4",
        "friends-of-contao/contao-associategroups": "~1.3@dev"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2",
        "contao/manager-plugin": "^2.1"
    },
    "autoload": {
        "psr-4": { "Netzmacht\\Contao\\Workflow\\": "src/" }
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "2.0.x-dev"
        }
    }
}
